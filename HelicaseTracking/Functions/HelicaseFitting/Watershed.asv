function [ outputImage ] = HelicaseWatershed( helicaseImage )
    % HelicaseWatershed - Returns an image of the helicases after using the watershed
    %   Detailed explanation goes here

    invertImage = uint32( abs(helicaseImage - max(helicaseImage(:))) );
    watershedImage = watershed(invertImage);
    
    figure; imshow(watershedImage , [] )
    

end

